{
	"version": 3,
	"configurePresets": [
		{
			"name": "dbg",
			"binaryDir": "build",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": {
					"type": "STRING",
					"value": "Debug"
				}
			}
		},
		{
			"name": "rel",
			"inherits": "dbg",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": {
					"type": "STRING",
					"value": "Release"
				}
			}
		}
	]
}
